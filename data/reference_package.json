  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "testcovFull": "export DEBUG=test&&jest --coverageReporters='text'",
    "test:watch": "export DEBUG=test&&jest --watchAll",
    "trial": "export DEBUG=test&&jest --collectCoverage=false --watchAll",
    "lint": "eslint src",
    "compile:watch": "tsc --build tsconfigs/test/tsconfig.json --watch",
    "compile": "tsc --build tsconfigs/build/tsconfig.json",
    "install": "npm run compile",
    "watch": "nodemon --ext ts,json --exec ",
    "backup": "./scripts/mongoexport.sh",
    "start": "node -r ts-node/register --max-old-space-size=3049"
